{
  "name": "mumbling-mole",
  "version": "0.5.1",
  "description": "An HTML5 Mumble client.",
  "scripts": {
    "build": "./smart-build.sh",
    "build:dev": "WEBPACK_MODE=development ./smart-build.sh",
    "build:force": "./smart-build.sh --force",
    "build:vendor:mumble-client": "npx babel vendors/mumble-client/src --out-dir vendors/mumble-client/lib",
    "audit:ci": "node scripts/audit-ci.cjs",
    "audit:baseline": "node scripts/audit-ci.cjs --write-baseline",
    "prepare": "if [ -z \"$SKIP_PREPARE\" ]; then rm -rf dist && mkdir -p dist && npm run build; fi",
    "test:e2e": "node scripts/e2e-check.cjs",
    "test": "npm run test:e2e && npm run audit:ci",
    "check:deps": "npx depcheck --config .depcheckrc",
    "analyze": "npx webpack --config webpack.analyze.config.js --json > analyze-stats.json 2>/dev/null && npx webpack-bundle-analyzer analyze-stats.json dist --mode static --report dist/bundle-report.html --no-open && echo 'Bundle analysis report generated at dist/bundle-report.html'"
  },
  "author": "Jens Fielenbach <flexpair@icloud.com>",
  "repository": "Flexpair/mumbling-mole",
  "homepage": "https://github.com/jafudi/flexpair",
  "engines": {
    "node": ">=22.0.0"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@babel/plugin-transform-runtime": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@babel/runtime": "^7.28.4",
    "audio-context": "1.0.3",
    "babel-loader": "10.0.0",
    "brfs": "2.0.2",
    "copy-webpack-plugin": "^13.0.1",
    "css-loader": "^7.1.2",
    "depcheck": "^1.4.7",
    "drop-stream": "1.0.0",
    "extract-loader": "^4.0.3",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.4",
    "keyboardjs": "2.7.0",
    "knockout": "3.5.1",
    "libopus.js": "0.0.1",
    "mini-css-extract-plugin": "2.9.4",
    "mumble-client": "file:vendors/mumble-client",
    "netlify-identity-widget": "file:vendors/netlify-identity-widget",
    "node-polyfill-webpack-plugin": "4.1.0",
    "promise": "8.3.0",
    "raw-loader": "4.0.2",
    "regexp-replace-loader": "1.0.1",
    "reuse-pool": "1.0.2",
    "sass": "1.93.2",
    "sass-loader": "16.0.5",
    "stream-chunker": "1.2.8",
    "to-arraybuffer": "1.0.1",
    "transform-loader": "0.2.4",
    "wait-port": "^1.1.0",
    "web-audio-buffer-queue": "1.1.0",
    "webpack": "^5.102.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "websocket-stream": "^5.5.2",
    "ws": "^8.18.3"
  },
  "overrides": {
    "ws": "^8.18.3"
  }
}
